                                </mod>
                            </mref>
                            modified as follow:
                            “<quotedText id="mmod2-qtd1" for="mmod2-mod1">copyright</quotedText>”
                            <omissis/>
                            “<quotedText id="mmod2-qtd2" for="mmod2-mod2">privacy</quotedText>”
